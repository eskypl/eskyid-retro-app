{
  "name": "retroanywhere",
  "version": "0.1.0",
  "description": "eSky Innovation Days Retro App",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "deploy": "npm run build && firebase deploy",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "devDependencies": {
    "awesome-typescript-loader": "1.1.1",
    "copy-webpack-plugin": "3.0.1",
    "file-loader": "0.9.0",
    "reflect-metadata": "0.1.3",
    "source-map-loader": "0.1.5",
    "typescript": "1.8.10",
    "typings": "1.3.0",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.14.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.7",
    "core-js": "2.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12",
    "firebase": "3.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eskypl/retroanywhere.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eskypl/retroanywhere/issues"
  },
  "homepage": "https://github.com/eskypl/retroanywhere#readme"
}
